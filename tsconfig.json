{
  "exclude": [
    "integration-tests",
    "lib"
  ],
  "include": [
    "node_modules/cypress",
  ],
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
